{
  "name": "mern-shop",
  "version": "1.0.0",
  "description": "E-Commerce Application with MERN Stack",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf -f ./public/* && rimraf -f ./serverBuild/*",
    "dev": "npm-run-all --parallel dev:* lint:watch",
    "dev:server": "nodemon --watch serverBuild --exec \"node serverBuild/server.bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "localtunnel": "lt --port 7000",
    "lint": "esw webpack.* src server",
    "lint:watch": "npm run lint -- --watch",
    "slate": "rimraf -rf ./node_modules && npm install",
    "security:check": "nsp check",
    "share:work": "npm-run-all --parallel dev localtunnel"
  },
  "author": "Mike",
  "license": "MIT",
  "dependencies": {
    "axios": "0.16.2",
    "compression": "1.7.1",
    "express": "4.16.2",
    "lodash": "4.17.4",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "serialize-javascript": "1.4.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "concurrently": "3.5.0",
    "eslint": "^4.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.3",
    "localtunnel": "^1.8.3",
    "nodemon": "1.12.1",
    "npm-run-all": "4.1.1",
    "nsp": "^2.8.1",
    "rimraf": "^2.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.2",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  }
}
